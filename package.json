{
  "name": "formcraft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "tunnel": "lt --port 3000 --subdomain formtunnel",
    "build:workers": "esbuild workers/*.ts --bundle --platform=node --target=node21 --outdir=workers/dist",
    "build:worker": "esbuild workers/${WORKER}-connector-worker.ts --bundle --platform=node --target=node21 --outdir=workers/dist",
    "start:worker:webhook-connector": "dotenv -e .env.local -- node workers/dist/webhook-connector-worker.js",
    "start:worker:sheets-connector": "dotenv -e .env.local -- node workers/dist/sheets-connector-worker.js",
    "start:worker:email-connector": "dotenv -e .env.local -- node workers/dist/email-connector-worker.js",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:migrate:create": "dotenv -e .env.local -- prisma migrate dev --create-only",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "prisma:migrate:reset": "dotenv -e .env.local -- prisma migrate reset --force",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "stripe:sync": "dotenv -e .env.local -- node ./scripts/sync_stripe.mjs",
    "postinstall": "pnpm prisma:generate",
    "lavinmq": "docker run --rm -it -p 5672:5672 -p 15672:15672 -v /tmp/amqp:/var/lib/lavinmq cloudamqp/lavinmq",
    "db:clean": "node ./scripts/clean_database.mjs"
  },
